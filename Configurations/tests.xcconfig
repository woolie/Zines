// Xcode settings for Woolsoft macOS test targets, debug configuration

#include "common.xcconfig"

ONLY_ACTIVE_ARCH = YES
GCC_OPTIMIZATION_LEVEL = 0
BITCODE_GENERATION_MODE = marker
ENABLE_NS_ASSERTIONS = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) DEBUG=1
SWIFT_COMPILATION_MODE = singlefile
SWIFT_OPTIMIZATION_LEVEL = -Onone
OTHER_SWIFT_FLAGS = -DDEBUG
MTL_ENABLE_DEBUG_INFO = INCLUDE_SOURCE
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
TEST_HOST = $(BUILT_PRODUCTS_DIR)/Zines.app/Zines
APPLICATION_EXTENSION_API_ONLY = NO
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
STRIP_INSTALLED_PRODUCT = NO
//STRIP_LINKED_PRODUCT YES		Xcode barfs on this do it in the target
INFOPLIST_FILE = ZinesTests/Info.plist
PRODUCT_BUNDLE_IDENTIFIER = com.woolsoft.ZinesTests
PRODUCT_NAME = ZinesTests
DEFINES_MODULE = NO
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES

DEVELOPMENT_TEAM = XXX
